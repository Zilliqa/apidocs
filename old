            </span><span class="s2">"Difficulty"</span><span class="p">:</span><span class="mi">3</span><span class="p">,</span><span class="w">
            </span><span class="s2">"DifficultyDS"</span><span class="p">:</span><span class="mi">16</span><span class="p">,</span><span class="w">
            </span><span class="s2">"GasPrice"</span><span class="p">:</span><span class="s2">"100"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"LeaderPubKey"</span><span class="p">:</span><span class="s2">"0x020C11D6B9CA45B56EC80705F1070D9928CFA4BA99212BEF934307C228ECC309FC"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"PoWWinners"</span><span class="p">:[</span><span class="w">
                </span><span class="s2">"0x02CB5328E79387F337EC671ECFF0CF2501CCD4DDFCA3565640F94792DE0A7EA0F9"</span><span class="p">,</span><span class="w">
                </span><span class="s2">"0x0320D26B67A1C701377AA391F2572B66D72F5D0E1A23C44E5B1DA7BF2D53C653E1"</span><span class="w">
            </span><span class="p">],</span><span class="w">
            </span><span class="s2">"PrevHash"</span><span class="p">:</span><span class="s2">"449a67f3cb160f598ac279f95e6583868a2052c846123558866f56dd6d70cd6c"</span><span class="p">,</span><span class="w">
